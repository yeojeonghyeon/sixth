<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>HelloWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>setCharacterEncodingFilter</filter-name>
    <filter-class>org.apache.catalina.filters.SetCharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>setCharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>HelloServlet</display-name>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>com.helloworld.servlet.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/HelloServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ClassOrStudentServlet</display-name>
    <servlet-name>ClassOrStudentServlet</servlet-name>
    <servlet-class>com.helloworld.servlet.ClassOrStudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClassOrStudentServlet</servlet-name>
    <url-pattern>/ClassOrStudentServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CookieMakingServlet</display-name>
    <servlet-name>CookieMakingServlet</servlet-name>
    <servlet-class>com.helloworld.servlet.CookieMakingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CookieMakingServlet</servlet-name>
    <url-pattern>/CookieMakingServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>FirstStepServlet</display-name>
    <servlet-name>FirstStepServlet</servlet-name>
    <servlet-class>com.helloworld.servlet.FirstStepServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FirstStepServlet</servlet-name>
    <url-pattern>/FirstStepServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>SecondStepServlet</display-name>
    <servlet-name>SecondStepServlet</servlet-name>
    <servlet-class>com.helloworld.servlet.SecondStepServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SecondStepServlet</servlet-name>
    <url-pattern>/SecondStepServlet</url-pattern>
  </servlet-mapping>  
</web-app>